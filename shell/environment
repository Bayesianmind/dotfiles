# Go language specific settings

if [ -z "${GOPATH}" ] ; then
  for dir in '.gocode' 'src/gocode' ; do
    if [ -d "$HOME/$dir" ]; then
      export GOPATH="$HOME/$dir"

      export PATH="$PATH:$HOME/$dir/bin"
    fi
  done
fi

# Enable e.g. `ls` colors
export CLICOLOR=1

# Prefer IPv4
export _JAVA_OPTIONS="${_JAVA_OPTIONS} -Djava.net.preferIPv4Stack=true"